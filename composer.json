{
    "name": "yoha/qr",
    "description": "This package is made to make qr generation easy",
    "type": "library",
    "minimum-stability": "stable",
    "scripts": {
        "test": "phpunit --bootstrap vendor/autoload.php tests",
        "lint": "phpstan analyse src --level=max"
    },
    "require": {
        "endroid/qr-code": "^6.0",
        "symfony/console": "^7.2",
        "symfony/process": "^7.2",
        "setasign/fpdf": "^1.8",
        "php": "^8.3.0"
    },
    "require-dev": {
        "symfony/var-dumper": "^7.2",
        "phpunit/phpunit": "^12.0",
        "phpstan/phpstan": "^2.1"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Yoha\\Qr\\": "src/",
            "Yoha\\Qr\\Tests\\": "tests/"
        },
        "files": [
            "src/functions/app.php"
        ]
    },
    "authors": [
        {
            "name": "Yohannes Zerihun",
            "email": "johnpro3269@gmail.com",
            "homepage": "http://yohacodes.pro.et",
            "role": "Developer"
        }
    ],
    "funding": [
        {
            "type": "github",
            "url": "https://github.com/sponsors/yohatechtop"
        }
    ],
    "suggest": {
        "ext-imagick": "For high-quality image processing and generation.",
        "ext-gd": "Alternative lightweight image processing extension."
    }
}