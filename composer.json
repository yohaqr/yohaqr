{
    "name": "yoha/qr",
    "description": "This package is made to make qr generation easy",
    "type": "library",
    "minimum-stability": "stable",
    "scripts": {
        "test": "vendor/bin/phpunit",
        "lint": "phpstan analyse app --level=max"
    },
    "require": {
        "endroid/qr-code": "^6.0",
        "symfony/console": "^7.2",
        "symfony/process": "^7.2",
        "setasign/fpdf": "^1.8"
    },
    "require-dev": {
        "symfony/var-dumper": "^7.2",
        "phpunit/phpunit": "^12.0",
        "phpstan/phpstan": "^2.1"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Yoha\\Qr\\": "app/",
            "Test\\": "tests/"
        },
        "files": ["app/functions/app.php"]
    },
    "authors": [
        {
            "name": "Yohannes Zerihun",
            "email": "johnpro3269@gmail.com",
            "homepage": "http://yohacodes.pro.et",
            "role": "Developer"
        }
    ]
}
